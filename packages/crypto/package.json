{
  "name": "@gdapi/crypto",
  "version": "0.1.3",
  "description": "Encrypt/Decrypt Geometry Dash related data",
  "main": "src/index.js",
  "jsdelivr": "build/index.js",
  "unpkg": "build/index.js",
  "type": "module",
  "scripts": {
    "build": "wireit",
    "build:test": "wireit",
    "test": "wireit",
    "lint": "wireit",
    "prepublishOnly": "wireit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GDAPI/GDAPI"
  },
  "keywords": [
    "geometrydash"
  ],
  "author": "Quasar-Kim",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GDAPI/GDAPI/issues"
  },
  "homepage": "https://github.com/GDAPI/GDAPI/tree/master/packages/crypto#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "concurrently": "^5.3.0",
    "jasmine-core": "^3.6.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^4.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.35.1",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "base64-js": "^1.5.1",
    "pako": "^2.0.2"
  },
  "wireit": {
    "build": {
      "command": "rollup -c rollup.prod.config.js",
      "files": [
        "src/**/*.js",
        "test/**/*.js"
      ],
      "output": [
        "build"
      ],
      "clean": true
    },
    "build:test": {
      "command": "rollup -c rollup.test.config.js",
      "files": [
        "src/**/*.js",
        "test/**/*.js"
      ],
      "output": [
        "build"
      ],
      "clean": true
    },
    "test": {
      "command": "karma start karma.config.cjs --single-run",
      "files": [
        "src/**/*.js",
        "test/**/*.js"
      ],
      "output": [],
      "dependencies": [
        "build:test"
      ]
    },
    "lint": {
      "command": "eslint --color --cache --cache-location .eslintcache .",
      "files": [
        "src/**/*.js",
        "test/**/*.js",
        ".eslintrc.cjs",
        ".eslintignore"
      ],
      "output": []
    },
    "prepublishOnly": {
      "dependencies": [
        "lint",
        "build",
        "test"
      ]
    }
  }
}
